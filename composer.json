{
    "name": "noot-pro/content-management",
    "description": "Simple CMS for your website. It includes posts, pages, tags, and categories.",
    "keywords": [
        "laravel",
        "lara-zeus",
        "cms",
        "pages",
        "posts",
        "menu",
        "news",
        "wordpress laravel",
        "wordpress alternative",
        "laravel cms",
        "filamentphp"
    ],
    "homepage": "https://larazeus.com/noot-pro/content-management",
    "support": {
        "issues": "https://github.com/lara-zeus/noot-pro/content-management/issues",
        "source": "https://github.com/lara-zeus/noot-pro/content-management"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lara Zeus (Ash)",
            "email": "info@larazeus.com"
        },
        {
            "name": "Ayman Alhattami",
            "email": "ayman.m.alhattami@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-tags-plugin": "^4.0",
        "lara-zeus/core": "^4.0",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-sluggable": "^3.3",
        "spatie/laravel-tags": "^4.6"
    },
    "require-dev": {
        "filament/upgrade": "4.0",
        "larastan/larastan": "^3.8",
        "laravel/pint": "^1.26",
        "orchestra/testbench": "^10.8",
        "pestphp/pest": "^4.2",
        "pestphp/pest-plugin-livewire": "^4.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "NootPro\\ContentManagement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NootPro\\ContentManagement\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NootPro\\ContentManagement\\ContentManagementServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
